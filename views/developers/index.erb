<h1>Developer Page</h1>

<table class="table">
  <thead>
    <th>ID</th>
    <th>Name</th>
    <th>Founded</th>
    <th>Country</th>
  </thead>
  <tbody>
    <% @developers.each do |developer| %>
      <tr>
        <td><%= developer.id %></td>
        <td><a href="/developers/<%= developer.id %>"><%= developer.name %></a></td>
        <td><%= developer.founded %></td>
        <td><%= developer.country%></td>
      </tr>
    <% end %>
  </tbody>
</table>
